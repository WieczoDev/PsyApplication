<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="629.0" prefWidth="607.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="psy_application.controller.CancelConsul">
    <children>
        <BorderPane layoutX="-2.0" layoutY="-1.0" prefHeight="615.0" prefWidth="609.0">
            <top>
                <Label alignment="CENTER" prefHeight="84.0" prefWidth="609.0" style="-fx-background-color: #000000;"
                       text="Annulation d'une consultation" textFill="WHITE" BorderPane.alignment="CENTER">
                    <font>
                        <Font name="System Italic" size="38.0"/>
                    </font>
                </Label>
            </top>
            <center>
                <Pane fx:id="container" prefHeight="572.0" prefWidth="609.0" BorderPane.alignment="CENTER">
                    <children>
                        <DatePicker fx:id="date_field" layoutX="218.0" layoutY="41.0"/>
                        <Label layoutX="181.0" layoutY="14.0" text="Chercher la consultation par rapport à la date :"/>
                        <Button fx:id="findButton" layoutX="322.0" layoutY="79.0" mnemonicParsing="false"
                                onAction="#findButtonAction" text="Chercher"/>
                        <Button fx:id="closeButton" layoutX="238.0" layoutY="79.0" mnemonicParsing="false"
                                onAction="#closeButtonAction" text="Annuler"/>
                        <TableView fx:id="tableview" layoutX="9.0" layoutY="142.0" prefHeight="322.0" prefWidth="592.0">
                            <columns>
                                <TableColumn fx:id="heureCol" prefWidth="75.0" text="Heure"/>
                                <TableColumn fx:id="idCol" prefWidth="139.0" text="Numéro de Consul"/>
                                <TableColumn fx:id="patient1Col" prefWidth="75.0" text="Patient n*1"/>
                                <TableColumn fx:id="patient2Col" prefWidth="75.0" text="Patient *2"/>
                                <TableColumn fx:id="patient3Col" prefWidth="75.0" text="Patient *3"/>
                                <TableColumn fx:id="reasonCol" prefWidth="68.0" text="Raison"/>
                                <TableColumn fx:id="rangeCol" prefWidth="84.0" text="Range"/>
                            </columns>
                        </TableView>
                    </children>
                </Pane>
            </center>
            <bottom>
                <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#handleDeletePerson" prefHeight="52.0"
                        prefWidth="223.0" text="Supprimer la ligne selectionné" BorderPane.alignment="CENTER"/>
            </bottom>
        </BorderPane>
    </children>
</AnchorPane>
